    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Windows",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "windows"
        },
        {
            "name": "Debug Web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "chrome",
            "args": [
                "--web-browser-flag",
                "--disable-web-security"
            ]
        },
        {
            "name": "Profile Windows",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "deviceId": "windows"
        },
        {
            "name": "Release Windows",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "windows"
        },
        {
            "name": "Release Web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "deviceId": "chrome",
            "args": [
                "--web-browser-flag",
                "--disable-web-security"
            ]
        }
    ]
}